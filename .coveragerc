# nosetests --with-coverage --cover-erase --cover-package .

# .coveragerc to control coverage.py
[run]
source = qustop
branch = True

[report]
show_missing = True
precision = 2
omit =
        *__init__*
        */tests/*
        examples/*
        *_about*
        *_version*

[html]
directory = coverage_html_report